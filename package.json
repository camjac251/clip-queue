{
  "name": "clip-queue",
  "version": "1.13.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "api": "turbo run --filter=@cq/api",
    "web": "turbo run --filter=@cq/web",
    "cfg": "turbo run --filter=@cq/config",
    "player": "turbo run --filter=@cq/player",
    "platforms": "turbo run --filter=@cq/platforms",
    "services": "turbo run --filter=@cq/services",
    "sources": "turbo run --filter=@cq/sources",
    "ui": "turbo run --filter=@cq/ui",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "typecheck": "turbo run typecheck",
    "deps:update": "corepack use pnpm@latest && pnpm up --latest && pnpm --recursive up --latest",
    "commitlint": "commitlint --edit",
    "dev:all": "turbo run dev --filter=@cq/api --filter=@cq/web"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "turbo": "^2.5.8",
    "typescript": "catalog:",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34",
  "engines": {
    "pnpm": ">=10",
    "node": ">=20"
  }
}
